<script type="text/javascript">

init();
var sequence;

var sequenceOptions = {
		autoPlay : false,
		nextButton : true,
		prevButton : true,
		animateStartingFrameIn : true,
		transitionThreshold : 250,
		keyNavigation: false,
		swipeNavigation : false
	}
	

  var mainHiden;
  
  
function init(){
  
  	
      console.log("init");
        sequence = $("#sequence").sequence(sequenceOptions).data("sequence");
        //sequence.init.preloader();
        
        window.addEventListener( 'resize', onResize, false );
        
				viewer = document.createElement( 'iframe' );
        //viewer = document.getElementById("viewer");        
				viewer.name = 'viewer';
				viewer.setAttribute( 'allowfullscreen', 'allowfullscreen' );
        viewer.setAttribute( 'allowtransparency', 'yes' );
        viewer.setAttribute( 'scrolling', 'no' );
				viewer.src = "/lab/streetart/";
				document.body.appendChild( viewer );
        
        viewer.onload= function() {
           $("#sequence").fadeOut();
            mainHiden = true;
        };
    
         
       
        onResize();
        
        
}


$('a[data-sequence]').click(function (e) {
		var sequenceid = $(e.target).data("sequence");

    console.log(mainHiden, sequenceid, sequence);
    
    if(mainHiden)
    {
      mainHiden = false;
      $("#sequence").fadeIn(
      function() {
        sequence.stopAutoPlay()
        sequence.next();
      });
    }else {
        sequence.stopAutoPlay()
        sequence.next();
    }
		
		
});
  
function onResize( event ) {

				viewer.style.width = window.innerWidth + 'px';
				viewer.style.height = ( window.innerHeight) + 'px';

			}
        
</script>



